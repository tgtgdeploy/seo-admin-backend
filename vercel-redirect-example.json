{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "globalinsighthub.xyz"
        }
      ],
      "destination": "https://telegramtghub.com/:path*",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "infostreammedia.xyz"
        }
      ],
      "destination": "https://telegramupdatecenter.com/:path*",
      "permanent": true,
      "statusCode": 301
    }
  ]
}
