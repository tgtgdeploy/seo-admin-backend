{
  "name": "seo-admin",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "cd packages/database && npm run db:generate && cd ../.. && next dev --port 3100",
    "build": "cd packages/database && npm run db:generate && cd ../.. && next build && node scripts/copy-prisma-engines.js",
    "start": "next start --port 3100",
    "lint": "next lint",
    "db:generate": "cd packages/database && npm run db:generate",
    "db:push": "cd packages/database && npm run db:push",
    "db:studio": "cd packages/database && npm run db:studio",
    "db:seed": "cd packages/database && npm run db:seed"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.62",
    "@tanstack/react-query": "^5.13.0",
    "@types/js-cookie": "^3.0.6",
    "@vercel/sdk": "^1.16.1",
    "ai": "^5.0.87",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.0.0",
    "js-cookie": "^3.0.5",
    "next": "14.2.33",
    "next-auth": "^4.24.5",
    "next-intl": "^4.4.0",
    "openai": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.0",
    "recharts": "^2.10.3",
    "zod": "^3.22.4"
  },
